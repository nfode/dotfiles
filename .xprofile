export LANG="en_US.UTF-8"

setxkbmap de
bin=$HOME/.local/bin
host=$(hostname)
if [ "$host" = "cranky-panini" ]; then
    xautolock -time 5 -locker $HOME/.local/bin/lock &
    nextcloud &
    redshift-gtk &
fi
compton -c &
$bin/launchbar &
$bin/setwallpaper &
nm-applet &
jetbrains-toolbox --minimize &
udiskie -2 &
xset -b

