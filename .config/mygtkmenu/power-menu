item = Log off
cmd = exit 
icon = /usr/share/icons/la-capitaine-icon-theme-0.5.0/actions/22x22-dark/application-exit.svg

item = Sleep
cmd = gotosleep
icon = /usr/share/icons/la-capitaine-icon-theme-0.5.0/actions/22x22-dark/system-suspend-hibernate.svg

item = Restart
cmd = systemctl reboot
icon = /usr/share/icons/la-capitaine-icon-theme-0.5.0/actions/22x22-dark/system-restart.svg

item = Shutdown
cmd = systemctl poweroff
icon = /usr/share/icons/la-capitaine-icon-theme-0.5.0/actions/22x22-dark/system-shutdown.svg
